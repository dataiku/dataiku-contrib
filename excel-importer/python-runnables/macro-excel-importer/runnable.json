{
    "meta": {
        "label": "Excel xlsx sheets importer",
        "description": "Creates one dataset per excel sheet",
        "icon": "icon-puzzle-piece"
    },
    "macroRoles": [
       {
           "type": "MANAGED_FOLDER",
           "targetParamsKey": "model_folder_id"
       },
       {
           "type": "PROJECT_MACROS",
           "targetParamsKey": "model_folder_id"
       }
   ],
   "params": [
       {
           "name": "model_folder_id",
           "type": "FOLDER",
           "label": "Folder name",
           "description": "Name of the folder that contains the xlsx file(s)"
       },
       {
          "name": "overwrite",
          "type": "BOOLEAN",
          "label": "Overwrite if exist",
          "default": false
       }],
    "resultType" : "RESULT_TABLE"
}

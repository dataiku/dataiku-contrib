{
   
    "meta" : {
        "label" : "DataRendezVous Exporter",
        "description" : "Export your dataset into DataRendezVous",
        "icon" : "icon-heart"
    },
    
 
    "exportBehavior" : "MANAGES_OUTPUT",
    
    
   
    "exportOptions": [
         {
            "label" : "Upload to DataRendezVous"
         }
    ],


    "params": [
        {
            "name": "s1",
            "label": "Configuration",
            "type": "SEPARATOR"
        },
        
        {
            "name": "api_key",
            "label" : "API Key",
            "type": "STRING",
            "mandatory" : true
        },

        {
            "name": "export_type",
            "label": "Export into...",
            "type": "SELECT",
            "description": "Place where your dataset will be exported.",
            "defaultValue": "create_dataset",
            "selectChoices": [
                /*{
                    "value": "create_datapot",
                    "label": "New datapot"
                },*/
                {
                    "value": "create_dataset",
                    "label": "New dataset"
                },
                {
                    "value": "create_version",
                    "label": "New dataset version"
                }
            ],
            "mandatory" : true
        },
        {
            "name": "datapot_id",
            "label" : "Datapot ID",
            "type": "STRING",
            "mandatory" : true
        },
        {
            "name": "dataset_id",
            "label" : "Dataset ID",
            "type": "STRING",
            "mandatory" : true,
            "visibilityCondition" : "model.export_type == 'create_version'"
        },
        {
            "name": "dataset_name",
            "label" : "Dataset name",
            "type": "STRING",
            "mandatory" : true,
            "visibilityCondition" : "model.export_type == 'create_dataset'"
        }
        
    ]
}
